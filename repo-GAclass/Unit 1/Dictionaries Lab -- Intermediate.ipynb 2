  {
   "cell_type": "code",
   "execution_count": 18,
   "metadata": {},
   "outputs": [],
   "source": [
    "cleaned_repos = []\n",
    "\n",
    "for repo in repos[1:]:\n",
    "    # if the repo has more than one character\n",
    "    if len(repo) > 1:\n",
    "        split_repo = repo.split('/')\n",
    "        if len(split_repo) == 4:\n",
    "            cleaned_repos.append(split_repo[-1])\n",
    "        elif len(split_repo) == 5:\n",
    "            cleaned_repos.append(split_repo[-2])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 19,
