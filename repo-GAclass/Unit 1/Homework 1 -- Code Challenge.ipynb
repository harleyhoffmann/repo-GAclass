    "    answer = \"\"\n",
    "    count = 0\n",
    "    #Add in first character\n",
    "    answer += input[0]\n",
    "\n",
    "    #Iterate through loop\n",
    "    for item in word:\n",
    "        compare = ord(item)\n",
    "        if temp != compare: #different character value\n",
    "            if count >= 1:\n",
    "                answer += str(count)\n",
    "            answer += item\n",
    "        \n",
    "        temp = ord(item)\n",
    "        count = 1\n",
    "    else:\n",
    "        count = count+1\n",
    "    answer += str(count)\n",
    "    return answer\n",
